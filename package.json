{
  "name": "@reactgular/layouts",
  "version": "0.0.0",
  "description": "Layouts is an Angular component library that implements a *border layout* using a container that can dock child panels into Top, Left, Right, Bottom  and Center regions.",
  "author": "reactgular.com",
  "homepage": "https://reactgular.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reactgular/layouts"
  },
  "bugs": {
    "url": "https://github.com/reactgular/layouts/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --open",
    "build": "npm run build:layouts && npm run build:demo",
    "build:watch": "ng build layouts --watch",
    "build:demo": "ng build demo --prod --deploy-url \"https://reactgular.github.io/layouts/\"",
    "build:layouts": "ng build layouts && cp README.md dist/layouts",
    "test": "ng test layouts --watch=false --code-coverage=true",
    "test:watch": "ng test layouts",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "~10.1.1",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.1",
    "@angular/compiler": "~10.1.1",
    "@angular/core": "~10.1.1",
    "@angular/forms": "~10.1.1",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "~10.1.1",
    "@angular/platform-browser-dynamic": "~10.1.1",
    "@angular/router": "~10.1.1",
    "@reactgular/destroyable": "^2.0.0",
    "@reactgular/logger": "^2.0.0",
    "@reactgular/observables": "^2.0.0",
    "@reactgular/stateful": "^1.0.3",
    "bootstrap": "^4.5.2",
    "faker": "^4.1.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.1",
    "@angular-devkit/build-ng-packagr": "~0.1001.1",
    "@angular/cli": "~10.1.1",
    "@angular/compiler-cli": "~10.1.1",
    "@angular/language-service": "~10.1.1",
    "@types/faker": "^4.1.10",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
